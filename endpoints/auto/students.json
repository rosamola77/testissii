{
    "getAll": {
        "description": "Gets all entries from 'students'",
        "route": "/students",
        "method": "GET",
        "sql": "SELECT * FROM students",
        "auth_required": false,
        "allowed_roles": [
            "*"
        ]
    },
    "getById": {
        "description": "Gets an entry from 'students' by its primary key",
        "route": "/students/$studentId",
